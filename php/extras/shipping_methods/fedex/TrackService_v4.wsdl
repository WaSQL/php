            </xs:annotation>
          </xs:element>
          <xs:element name="Packages" type="ns:TrackNotificationPackage" minOccurs="0" maxOccurs="unbounded">
            <xs:annotation>
              <xs:documentation>Data pertaining to packages.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
