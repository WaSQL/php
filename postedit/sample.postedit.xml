<settings>
	<!-- sound tag will say things -->
	<sound
		success="Ok"
		fail="Error!"
		gender="m"
	/>
	<!-- editor tag will run the command on success.-->
	<editor
		command="subl --add"
	/>
</settings>
<hosts>
	<!-- create a host entry for each domain and subdomain.
		Host attributes:
			name - host name
			alias - alias name to call the directory files go into
			username - username on this
			apikey - apikey of username
			tables - defaults to getting _models, _pages, _tables
			groupby - name,field, type, or extension.  Groups files by file name, field(body,controller,functions,js,css), type(models, views, controllers), or extension (html,php,js,css)
			datestamp - if 1 then the folder will be stamped with the date  (localhost_20200112)
			days_to_keep - number of days to keep datestamped folders.  Folders older than this will be deleted.
	-->
	<host
		name="localhost"
		alias="localhost"
		apikey="YWR6adZEVSMzNnasfdhZHBleHpnMWkFrOmFkcGV4easfdsaRlVaQWs="
		username="admin"
		groupby="name"
		insecure="1"
		datestamp="1"
		days_to_keep="30"
	/>
</hosts>

